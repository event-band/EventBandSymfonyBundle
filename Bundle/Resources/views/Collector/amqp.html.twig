{% extends app.request.isXmlHttpRequest ? 'WebProfilerBundle:Profiler:ajax_layout.html.twig' : 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
    <img width="20" height="28" alt="EventBand AMQP" src="data:image/png;base64,"/>
    <span class="sf-toolbar-status">{{ collector.publicationCount }}</span>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon"><img src="data:image/png;base64," alt="" /></span>
    <strong>EventBand AMQP</strong>
    <span class="count">
        <span>{{ collector.publicationCount }}</span>
    </span>
</span>
{% endblock %}

{% block panel %}

    <h2>EventBand AMQP</h2>

    <ul class="alt">
    {% for i, entry in collector.publications %}
        <li class="{{ cycle(["odd", "even"], i) }}">
            <div>
                <code>{{ entry.publication.message.body }}</code>
            </div>
            <small>
                <strong>Exchange</strong>: {{ entry.exchange }}<br />
                <strong>Routing key</strong>: {{ entry.routingKey }}
            </small>
        </li>
    {% endfor %}
    </ul>

{% endblock %}
